#!/sbin/busybox sh

if [ "a$2" != "a" ];then
  gestures7=$2
fi

case "${gestures7}" in
  app1)
	pkill -f "6gestures.sh"
	pkill -f "/sys/devices/virtual/misc/touch_gestures/wait_for_gesture"
	sed -i 's/app7=".*"/app7="com.android.browser\/.BrowserActivity"/g' /data/.kuban/6gestures.sh
	sed -i 's/ges7=".*"/ges7="on"/g' /data/.kuban/6gestures.sh
	/sbin/busybox sh /data/.kuban/6gestures.sh
	sed -i 's/gestures7=.*/gestures7=$2/g' /data/.kuban/default.profile
