	sed -i 's/gestures7=.*/gestures7=$2/g' /data/.kuban/default.profile
    ;;
  data)
	pkill -f "6gestures.sh"
	pkill -f "/sys/devices/virtual/misc/touch_gestures/wait_for_gesture"
	sed -i 's/app7=".*"/app7="data"/g' /data/.kuban/6gestures.sh
	sed -i 's/ges7=".*"/ges7="on"/g' /data/.kuban/6gestures.sh
	/sbin/busybox sh /data/.kuban/6gestures.sh
	sed -i 's/gestures7=.*/gestures7=$2/g' /data/.kuban/default.profile
    ;;
  wifi)
	pkill -f "6gestures.sh"
	pkill -f "/sys/devices/virtual/misc/touch_gestures/wait_for_gesture"
	sed -i 's/app7=".*"/app7="wifi"/g' /data/.kuban/6gestures.sh
	sed -i 's/ges7=".*"/ges7="on"/g' /data/.kuban/6gestures.sh
	/sbin/busybox sh /data/.kuban/6gestures.sh
	sed -i 's/gestures7=.*/gestures7=$2/g' /data/.kuban/default.profile
    ;;
  wimax)
	pkill -f "6gestures.sh"
	pkill -f "/sys/devices/virtual/misc/touch_gestures/wait_for_gesture"
	sed -i 's/app7=".*"/app7="wimax"/g' /data/.kuban/6gestures.sh
	sed -i 's/ges7=".*"/ges7="on"/g' /data/.kuban/6gestures.sh
	/sbin/busybox sh /data/.kuban/6gestures.sh
	sed -i 's/gestures7=.*/gestures7=$2/g' /data/.kuban/default.profile
    ;;
  off)
	pkill -f "6gestures.sh"
	pkill -f "/sys/devices/virtual/misc/touch_gestures/wait_for_gesture"
	sed -i 's/ges7=".*"/ges7="off"/g' /data/.kuban/6gestures.sh
	/sbin/busybox sh /data/.kuban/6gestures.sh
	sed -i 's/gestures1=.*/gestures1=$2/g' /data/.kuban/default.profile
	;;
esac;

echo $gestures7